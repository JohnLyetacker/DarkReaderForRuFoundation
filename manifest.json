{
    "manifest_version": 2,
    "name": "D R F R F",
    "version": "0.1",
  
    "description": "Dark reader for RuFoundation. Adds specific dark theme for scpfoundation.net.",
  
    "icons": {
      "48": "icons/interior.png"
    },

    "browser_specific_settings": {
        "gecko": {
          "id": "D-R-F-R-F@github.com/JohnLyetacker"
        }
      },
  
    "content_scripts": [
      {
        "matches": ["*://*.scpfoundation.net/*"],
        "js": ["drfrf.js"]
      }
    ]
  }
  